<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::content">
                    <resource id="Infobeans_Faq::faq_content" title="FAQ" sortOrder="10" >
                        <resource id="Infobeans_Faq::faq" title="FAQs" sortOrder="40" />                            
                         <resource id="Infobeans_Faq::category" title="Category" sortOrder="40" /> 
                    </resource> 
                </resource> 
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Infobeans_Faq::config" title="FAQ Config" sortOrder="50" />
                        </resource>
                    </resource>
                </resource>
            </resource> 
        </resources>
    </acl>
</config>